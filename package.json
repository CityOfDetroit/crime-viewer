{
  "name": "crime-viewer",
  "version": "1.0.0",
  "description": "View, filter and summarize incident data from the police department.",
  "main": "index.js",
  "scripts": {
    "watch": "watchify src/main.js -t babelify -o public/bundle.js -v",

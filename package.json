  "bugs": {
    "url": "https://github.com/CityOfDetroit/crime-viewer/issues"
  },
  "homepage": "https://github.com/CityOfDetroit/crime-viewer",
  "dependencies": {
    "chartist": "^0.11.0",
    "jq-accordion": "^1.0.1",
    "jquery": "^3.2.1",
    "jquery.scrollbar": "^0.2.11",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.38.0",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "slideout": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
